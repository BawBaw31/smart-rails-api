{% extends 'base.html.twig' %}

{% block title %}VisitType index{% endblock %}

{% block body %}
    <h1>VisitType index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for visit_type in visit_types %}
            <tr>
                <td>{{ visit_type.id }}</td>
                <td>{{ visit_type.label }}</td>
                <td>
                    <a href="{{ path('app_visit_type_show', {'id': visit_type.id}) }}">show</a>
                    <a href="{{ path('app_visit_type_edit', {'id': visit_type.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_visit_type_new') }}">Create new</a>
{% endblock %}
